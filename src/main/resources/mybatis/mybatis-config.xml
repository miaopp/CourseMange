<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC
        "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <settings>
        <setting name="useGeneratedKeys" value="true"/>
    </settings>

    <typeAliases>
        <typeAlias type="com.mpp.model.Apply" alias="Apply"/>
        <typeAlias type="com.mpp.model.Course" alias="Course"/>
        <typeAlias type="com.mpp.model.Lab" alias="Lab"/>
        <typeAlias type="com.mpp.model.User" alias="User"/>
    </typeAliases>

    <mappers>
        <mapper resource="mybatis/mapper/apply.xml"/>
        <mapper resource="mybatis/mapper/course.xml"/>
        <mapper resource="mybatis/mapper/lab.xml"/>
        <mapper resource="mybatis/mapper/user.xml"/>
    </mappers>
</configuration>