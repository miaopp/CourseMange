<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Lab">
    <resultMap id="labResultMap" type="Lab">
        <result property="id" column="lab_id"/>
        <result property="lab_name" column="lab_name"/>
        <result property="lab_address" column="lab_address"/>
        <result property="lab_dept" column="lab_dept"/>
    </resultMap>
    <insert id="insertLab" parameterType="Lab">
        <![CDATA[
            insert into lab
            (lab_name, lab_address, lab_dept)
            values (#{labname}, #{address}, #{labdept})
        ]]>
    </insert>
    <select id="selectLabs" resultType="Lab">
        <![CDATA[
            select *
            from lab;
        ]]>
    </select>
</mapper>